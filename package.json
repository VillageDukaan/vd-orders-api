{
  "name": "vd-orders-api",
  "version": "1.0.0",
  "engines": {
    "node": "10.15"
  },
  "main": "index.js",
  "repository": "git@github.com:sourcier/vd-orders-api.git",
  "author": "Roger Rajaratnam <roger@sourcier.uk>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exitcrash --exec babel-node ./src/server.js",
    "start:prod": "node -r appoptics-apm -r newrelic ./dist/server.js",
    "build": "babel src -d dist",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "precommit": "npm run lint",
    "test": "jest",
    "postinstall": "yarn build",
    "unused-deps": "yarn yarn-check -p"
  },
  "dependencies": {
    "@sentry/node": "^4.5.3",
    "apollo-server-express": "^2.3.3",
    "appoptics-apm": "^5.9.5",
    "config": "^3.0.1",
    "express": "^4.16.4",
    "firebase": "^5.8.1",
    "firebase-admin": "^6.5.1",
    "graphql": "^14.1.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "newrelic": "^5.2.1",
    "pg": "^7.8.0",
    "rollbar": "^2.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "yarn-check": "^0.0.3"
  }
}
