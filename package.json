{
  "name": "vd-orders-api",
  "version": "1.0.0",
  "engines": {
    "node": "10.15"
  },
  "main": "index.js",
  "repository": "git@github.com:sourcier/vd-orders-api.git",
  "author": "Roger Rajaratnam <roger@sourcier.uk>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec babel-node ./src/server.js",
    "start:prod": "node -r newrelic ./dist/server.js",
    "build": "babel src -d dist",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "test": "jest",
    "postinstall": "yarn build",
    "unused-deps": "yarn yarn-check -p"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.3",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "newrelic": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "yarn-check": "^0.0.3"
  }
}
